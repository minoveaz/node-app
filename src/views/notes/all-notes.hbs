<div class="row">
    {{#each notes}}
        <div class="col-md-3">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title d-flex justify-content-between align-items-center">
                        {{tittle}} 
                        <a href="#">
                            <i class="fas fa-bell"></i>
                        </a>
                    </h4>
                    <p>{{description}}</p>
                    <button class="btn btn-danger btn-block btn-sm">
                        delete
                    </button>
                </div>
            </div>
        </div>
    {{else}}
        <div class="card mx-auto">
            <div class="card-body">
                
            </div>
        </div>
    {{/each}}
</div>